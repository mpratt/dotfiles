<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/3.4/menu">
	<menu id="apps-net-menu" label="Internet">
		<item label="Firefox">
			<action name="Execute">
				<command>
                    firefox
                </command>
				<startupnotify>
					<enabled>
                        yes
                    </enabled>
					<wmclass>
                        Firefox
                    </wmclass>
				</startupnotify>
			</action>
		</item>
		<item label="Opera">
			<action name="Execute">
				<command>
                    opera
                </command>
				<startupnotify>
					<enabled>
                       yes
                    </enabled>
					<wmclass>
                       Opera
                    </wmclass>
				</startupnotify>
			</action>
		</item>
		<item label="Chrome (Incognito)">
			<action name="Execute">
				<execute>chromium --incognito</execute>
				<startupnotify>
					<enabled>
                        yes
                    </enabled>
				</startupnotify>
			</action>
		</item>
		<item label="GFTP">
			<action name="Execute">
				<execute>
                    gftp
                </execute>
			</action>
		</item>
		<separator/>
		<item label="Ktorrent">
			<action name="Execute">
				<execute>ktorrent</execute>
			</action>
		</item>
		<item label="Xchat">
			<action name="Execute">
				<execute>
                    xchat
                </execute>
			</action>
		</item>
	</menu>
