	<menu id="system-menu" label="System">
		<item label="Turn Off Screen">
			<action name="Execute">
				<execute>
                    xset dpms force off
                </execute>
			</action>
		</item>
		<item label="TaskManager">
			<action name="Execute">
				<execute>
                    xfce4-taskmanager
                </execute>
			</action>
		</item>
		<item label="Openbox Menu">
			<action name="Execute">
				<execute>
                    obmenu
                </execute>
			</action>
		</item>
		<item label="Openbox Config">
			<action name="Execute">
				<command>
                    obconf
                </command>
				<startupnotify>
					<enabled>
                        yes
                    </enabled>
				</startupnotify>
			</action>
		</item>
		<separator/>
		<item label="GTK Themes">
			<action name="Execute">
				<execute>
                    gtk-chtheme
                </execute>
			</action>
		</item>
		<item label="KDE System Settings">
			<action name="Execute">
				<execute>
                    kdesu systemsettings
                </execute>
			</action>
		</item>
		<item label="CUPs Printers">
			<action name="Execute">
				<command>
                    xdg-open http://localhost:631/
                </command>
				<startupnotify>
					<enabled>
                        no
                    </enabled>
					<icon>
                        cups
                    </icon>
				</startupnotify>
			</action>
		</item>
		<separator/>
		<item label="Reconfigure Openbox">
			<action name="Reconfigure"/>
		</item>
		<item label="Lock Screen">
			<action name="Execute">
				<execute>
                   xlock
                </execute>
			</action>
		</item>
		<item label="Shutdown / Reboot">
			<action name="Execute">
                <execute>obshutdown -c ~/.config/openbox/obshutdown.rc</execute>
			</action>
		</item>
	</menu>
	<menu id="root-menu" label="Openbox 3">
		<separator label="Application Menu"/>
		<menu id="apps-net-menu"/>
		<menu id="apps-multimedia-menu"/>
		<menu id="apps-office-menu"/>
		<menu id="apps-games-menu"/>
		<menu id="development-menu-link"/>
		<menu id="menu-apps-misc"/>
		<item label="Run Application">
			<action name="Execute">
				<execute>
                   gmrun
                </execute>
			</action>
		</item>
		<separator label="Common Apps"/>
		<item label="Vim">
			<action name="Execute">
				<execute>
                    gvim
                </execute>
			</action>
		</item>
		<item label="Terminal">
