				<execute>xfce4-terminal</execute>
			</action>
		</item>
		<item label="Thunar">
			<action name="Execute">
				<execute>
                   ck-launch-session dbus-launch --exit-with-session thunar
                </execute>
			</action>
		</item>
		<item label="Chromium">
			<action name="Execute">
				<execute>chromium</execute>
			</action>
		</item>
		<item label="Thunderbird">
			<action name="Execute">
				<decor>
                   yes
                </decor>
				<fullscreen>
                   no
                </fullscreen>
				<execute>
                   thunderbird
                </execute>
			</action>
		</item>
		<separator label="System Tools"/>
		<menu id="system-menu"/>
	</menu>
